{
  "$schema": "https://framework-1258016615.tcloudbaseapp.com/schema/latest.json",
  "version": "2.0",
  "envId": "{{envId}}",
  "framework": {
    "name": "vuepress-blog",
    "plugins": {
      "vuepress": {
        "use": "@cloudbase/framework-plugin-website",
        "inputs": {
          "buildCommand": "npm run build",
          "outputPath": ".vuepress/dist",
          "cloudPath": "/"
        }
      }
    }
  }
}
